{
  "name": "cndp-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "node ./server/server.js",
    "build": "next build & next export",
    "start": "NODE_ENV=production node ./server/server.js",
    "pm2:dev": "pm2 start npm --name 'youpin-portal' -- run dev",
    "lint": "next lint",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@koa/router": "^10.1.1",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "dayjs": "^1.10.7",
    "http-proxy": "^1.18.1",
    "justified-layout": "^4.1.0",
    "koa": "^2.13.4",
    "lodash.groupby": "^4.6.0",
    "lodash.omitby": "^4.6.0",
    "next": "12.0.7",
    "path-to-regexp": "^6.2.0",
    "qiankun": "^2.6.3",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.45.1",
    "stream-array": "^1.1.2",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/lodash.groupby": "^4.6.6",
    "@types/node": "17.0.1",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "path": "^0.12.7",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "locales/**/*.{js,ts}": [
      "node scripts/checkRepeat.js"
    ]
  },
  "engines": {
    "node": ">= 12.18.3"
  }
}
